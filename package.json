{
  "name": "try-to-js",
  "version": "1.0.3",
  "description": "try/catch wrapper for easy error handling in js",
  "keywords": [
    "node",
    "try",
    "catch",
    "try catch"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/AliReza99/try-to-js.git"
  },
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "author": "Alireza Bahrani",
  "license": "MIT",
  "private": false,
  "scripts": {
    "lint:prettier": "prettier --check --cache ./src/**/*.{js,jsx,ts,tsx}",
    "prettify": "prettier --write --cache ./src/**/*.{js,jsx,ts,tsx}",
    "lint": "tsc --noEmit && eslint --cache --cache-location ./node_modules/.cache/eslint/ --quiet --ext .js,.jsx,.ts,.tsx,.mjs src",
    "prebuild": "rimraf dist",
    "build": "tsc",
    "do-link": "npm link",
    "release:patch": "npm version patch",
    "release:major": "npm version major",
    "release:private": "npm publish --access=restricted",
    "release:public": "npm publish --access=public"
  },
  "dependencies": {},
  "devDependencies": {
    "@types/node": "^22.7.0",
    "typescript": "^5.6.2",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.48.0",
    "rimraf": "^6.0.1",
    "prettier": "^3.0.3"
  }
}
